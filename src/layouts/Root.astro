---
export const prerender = true
import Head, { type Props as HeaderProps } from "../components/Head.astro"


export interface Props extends Partial<HeaderProps> {}

const props = Astro.props;
---

<!DOCTYPE html>
<html
	lang="en"
	class={`
		min-h-screen
		min-h-[100dvh]
		bg-gradient-to-t
		bg-[#060a19]
		text-slate-50
	`}
>
	<head>
		<Head {...props as HeaderProps} />
	</head>
	<slot />
</html>
